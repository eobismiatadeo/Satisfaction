<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights#"
     xml:base="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:SatisfactionWithFlights="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights">
        <rdfs:comment xml:lang="en">This ontology measures the degree of satisfaction of the people surveyed with air travel.

It is created by Alejandro Tadeo Andreu and Eric Obis Moreno</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->


    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasCompany -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasCompany">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Company"/>
        <rdfs:comment>the flight is operated by a company</rdfs:comment>
        <rdfs:label>hasCompany</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasCountry -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasCountry">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Country"/>
        <rdfs:comment>The traveler is from a country</rdfs:comment>
        <rdfs:label>hasCountry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasDate -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Date"/>
        <rdfs:comment>The review from the traveler has date</rdfs:comment>
        <rdfs:label>hasDate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasDateFlight -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasDateFlight">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Date"/>
        <rdfs:comment>The flight has a date</rdfs:comment>
        <rdfs:label>hasDateFlight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasFlight -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasFlight">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:comment xml:lang="en">The review is from one flight</rdfs:comment>
        <rdfs:label xml:lang="en">hasFlight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasPlane -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasPlane">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Company"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Plane"/>
        <rdfs:comment>Company has plane for flights</rdfs:comment>
        <rdfs:label>hasPlane</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasRating -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Rating"/>
        <rdfs:comment>The review has a rating valoration</rdfs:comment>
        <rdfs:label>hasRating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasRecommended -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasRecommended">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Recomended"/>
        <rdfs:comment>Is the flight recommended</rdfs:comment>
        <rdfs:label>hasRecommended</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasReview -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasReview">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasTraveler"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:comment>The traveler make a review for his flight</rdfs:comment>
        <rdfs:label>hasReview</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasSeat -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasSeat">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Seat"/>
        <rdfs:comment>The traveler do a review from  a Seat for the travel.</rdfs:comment>
        <rdfs:label>hasSeat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasTravelPlane -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasTravelPlane">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Plane"/>
        <rdfs:comment xml:lang="en">The plane from the travel</rdfs:comment>
        <rdfs:label xml:lang="en">hasTravelPlane</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasTraveler -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasTraveler">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <rdfs:comment>The review has a traveler</rdfs:comment>
        <rdfs:label>hasTraveler</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasVerified -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasVerified">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Verified"/>
        <rdfs:comment>The review is verified</rdfs:comment>
        <rdfs:label>hasVerified</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/body -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/body">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/BodyReview"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Description of the review</rdfs:comment>
        <rdfs:label xml:lang="en">body</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/day -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/day">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Day 1 to 31</rdfs:comment>
        <rdfs:label xml:lang="en">day</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/destination -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/destination">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Destination of the flight</rdfs:comment>
        <rdfs:label xml:lang="en">destination</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/modelAirbus -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/modelAirbus">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Airbus"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Model of plane</rdfs:comment>
        <rdfs:label xml:lang="en">modelAirbus</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/modelBoeing -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/modelBoeing">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Boeing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">model of plane</rdfs:comment>
        <rdfs:label xml:lang="en">modelBoeing</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/month -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/month">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Month of the travel</rdfs:comment>
        <rdfs:label xml:lang="en">month</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/name -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/name">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name of traveler or name of country</rdfs:comment>
        <rdfs:label xml:lang="en">nameTraveler</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/nameCountry -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/nameCountry">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Country"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name of the country</rdfs:comment>
        <rdfs:label xml:lang="en">nameCountry</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/origin -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/origin">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Flight"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Name of the city origin the travel</rdfs:comment>
        <rdfs:label xml:lang="en">origin</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/ratingValue -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/ratingValue">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Rating"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Rating Value</rdfs:comment>
        <rdfs:label xml:lang="en">ratingValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/statVerify -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/statVerify">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Verified"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This is the state that the review is verified or not</rdfs:comment>
        <rdfs:label xml:lang="en">statVerify</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/surname -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/surname">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Surname of the traveler</rdfs:comment>
        <rdfs:label xml:lang="en">surname</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/title -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/title">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Title of the review</rdfs:comment>
        <rdfs:label xml:lang="en">title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/value -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/value">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Recomended"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Value true or false if the travel is recommended</rdfs:comment>
        <rdfs:label xml:lang="en">recomendedValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/year -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/year">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Date"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Year of the flight</rdfs:comment>
        <rdfs:label xml:lang="en">year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Airbus -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Airbus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Plane"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Boeing"/>
        <rdfs:comment>Is the manufacturer of the plane</rdfs:comment>
        <rdfs:label>Airbus</rdfs:label>
        <rdfs:sameAs>https://dbpedia.org/page/Airbus</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/BodyReview -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/BodyReview">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Review"/>
        <rdfs:comment>is the body of the review</rdfs:comment>
        <rdfs:label>BodyReview</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Boeing -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Boeing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Plane"/>
        <rdfs:comment>is the manufacturer of the plane</rdfs:comment>
        <rdfs:label>Boeing</rdfs:label>
        <rdfs:sameAs>https://dbpedia.org/page/Boeing</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Business -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Business">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Traveler"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/CoupleLeisure"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/FamilyLeisure"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/SoloLeisure"/>
        <rdfs:comment>travel for Business</rdfs:comment>
        <rdfs:label>Business</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/BusinessClass -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/BusinessClass">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Seat"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/EconomyClass"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/FirstClass"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/PremiumEconomy"/>
        <rdfs:comment>Seat in business class</rdfs:comment>
        <rdfs:label>BusinessClass</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Company -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Company">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/hasPlane"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Plane"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This is an airline company</rdfs:comment>
        <rdfs:label xml:lang="en">Company</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Country -->


    <owl:Class rdf:about="http://www.semanticweb.org/eric/ontologies/2024/3/SatisfactionWithFlights/Country">
        <rdfs:comment>Is the country of the traveler</rdfs:comment>
